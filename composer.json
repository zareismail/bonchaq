{
    "name": "zareismail/bonchaq",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "zareismail/nova-cascade": "~0.1",
        "zareismail/nova-currency": "~0.1",
        "zareismail/glance": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Bonchaq\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Bonchaq\\BonchaqServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true 
}
